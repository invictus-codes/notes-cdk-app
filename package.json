{
  "name": "@invictus-codes/notes-cdk-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/invictus-codes/notes-cdk-app"
  },
  "engines": {
    "node": ">=18.10.0",
    "pnpm": ">=7.20.0"
  },
  "private": true,
  "scripts": {
    "cdk": "cd packages/infra && pnpm cdk",
    "build:backend": "cd packages/backend && pnpm build",
    "prepare:frontend": "npx ts-node --esm --prefer-ts-exts packages/scripts/populate-frontend-config.ts",
    "build:frontend": "cd packages/frontend && pnpm build",
    "start:frontend": "cd packages/frontend && pnpm start",
    "serve:frontend": "cd packages/frontend && pnpm build && pnpm serve"
  }
}
