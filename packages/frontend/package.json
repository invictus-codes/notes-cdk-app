{
  "name": "@invictus-codes/notes-cdk-app-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare:frontend": "cd .. && pnpm prepare:frontend",
    "build": "tsc --noEmit && vite build",
    "build:frontend": "cd .. && pnpm build:frontend",
    "start": "vite",
    "start:frontend": "cd .. && pnpm start:frontend",
    "serve": "vite preview",
    "serve:frontend": "cd .. && pnpm serve:frontend",
    "cdk": "cd .. && pnpm cdk",
    "lint": "tsc --noEmit || eslint ./src --ext .js,.jsx,.ts,.tsx,.vue --cache",
    "lint:fix": "tsc --noEmit || eslint ./src --ext .js,.jsx,.ts,.tsx,.vue --cache --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "3.245.0",
    "@aws-sdk/client-polly": "3.245.0",
    "@aws-sdk/client-s3": "3.245.0",
    "@aws-sdk/client-transcribe-streaming": "3.245.0",
    "@aws-sdk/credential-provider-cognito-identity": "3.245.0",
    "@aws-sdk/polly-request-presigner": "3.245.0",
    "@aws-sdk/s3-request-presigner": "3.245.0",
    "@aws-sdk/util-create-request": "3.234.0",
    "@aws-sdk/util-format-url": "3.226.0",
    "@reach/router": "1.3.4",
    "buffer": "6.0.3",
    "events": "^3.3.0",
    "microphone-stream": "6.0.1",
    "process": "0.11.10",
    "react": "18.2.0",
    "react-bootstrap": "1.4.0",
    "react-dom": "18.2.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": ">=0.37.0",
    "eslint": "^8.36.0",
    "@types/node": "^18.11.18",
    "@types/reach__router": "1.3.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "react-bootstrap-icons": "1.3.0",
    "typescript": "~4.9.4",
    "vite": "3.2.5"
  }
